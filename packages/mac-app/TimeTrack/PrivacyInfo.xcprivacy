    <array>
        <!-- No tracking domains - TimeTrack respects user privacy -->
    </array>
    
    <!-- Data Use -->
    <key>NSPrivacyCollectedDataUse</key>
    <dict>
        <key>NSPrivacyCollectedDataUseDescription</key>
        <string>TimeTrack collects only the data necessary to provide time tracking functionality, including user account information for authentication and time tracking data for service provision. We do not track users or sell personal information.</string>
    </dict>
